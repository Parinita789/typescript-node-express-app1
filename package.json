{
  "name": "express-es6-rest-api",
  "version": "0.3.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "prestart": "npm run -s build",
    "start": "tsc && node dist/index.js",
    "test": "tsc && node src/test/*.ts"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/developit/express-es6-rest-api.git"
  },
  "author": "Parinita Kumari",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@types/chai": "^4.2.22",
    "@types/node": "^16.9.6",
    "@types/redis": "^2.8.32",
    "body-parser": "^1.13.3",
    "@types/express": "^4.16.1",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "inversify": "^5.1.1",
    "morgan": "^1.8.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "typescript": "^4.4.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@types/mocha": "^9.0.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "tslint": "^6.1.3"
  },
  "bugs": {
    "url": "https://github.com/developit/express-es6-rest-api/issues"
  },
  "homepage": "https://github.com/developit/express-es6-rest-api#readme"
}
